{
    "name": "marellocommerce/marello-enterprise",
    "description": "Marello Enterprise ERP for Commerce",
    "homepage": "https://github.com/marellocommerce/marello-enterprise.git",
    "license": "Commercial",
    "authors": [
        {
            "name": "Madia B.V.",
            "homepage": "http://www.marello.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "MarelloEnterprise\\Bundle": "src/"
        }
    },
    "repositories": {
        "oro": {
            "type": "composer",
            "url": "https://packagist.orocrm.com"
        },
        "platform-enterprise": {
            "type": "vcs",
            "url": "https://github.com/oroinc/platform-enterprise.git"
        },
        "asset-packagist": {
            "type": "composer",
            "url": "https://asset-packagist.orocrm.com"
        }
    },
    "require": {
        "php": "7.1.*",
        "oro/platform-enterprise": "2.6.*",
        "marellocommerce/marello": "1.5.*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.3.x-dev"
        }
    }
}